{
  "id": "connectivity-rerun",
  "commandLine": "bash run_connect.sh",
	"outputFiles": [
	  {
    "destination": {
      "container": {
        "containerUrl": "<SASURL>",
        "path": "logs"
        }
      },
    "filePattern": "*.out",
    "uploadOptions": {
      "uploadCondition": "taskcompletion"
      }
    },
	  {
		"destination": {
			"container": {
				"containerUrl": "<SASURL>"
			}
		},
		"filePattern": "outputs/.*",
		"uploadOptions": {
			"uploadCondition": "taskcompletion"
		}
	}
	],

	"resourceFiles": [{
		"autoStorageContainerName": "sendicott"
	}

	],

	"containerSettings": {
    "imageName": "rocker/r2u:jammy",
    "containerRunOptions": "--rm"
	},
    "userIdentity": {
        "autoUser": {
            "scope": "pool",
            "elevationLevel": "admin"
        }
    }
}
